    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@next-auth/firebase-adapter": "0.0.2-canary.207",
    "@stoplight/elements": "^9.0.2",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.81.5",
    "core-js": "^3.43.0",
    "daisyui": "^5.0.43",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.4.0",
    "googleapis": "^150.0.1",
